{
  "name": "serpent",
  "version": "1.0.1",
  "displayName": "serpent",
  "description": "Serpent language support and linting",
  "publisher": "beaugunderson",
  "repository": "https://github.com/beaugunderson/vscode-serpent",
  "homepage": "https://github.com/beaugunderson/vscode-serpent#readme",
  "bugs": "https://github.com/beaugunderson/vscode-serpent/issues",
  "license": "MIT",
  "engines": {
    "vscode": "^1.8.0"
  },
  "categories": [
    "Linters"
  ],
  "keywords": [
    "lint",
    "serpent",
    "serplint"
  ],
  "activationEvents": [
    "onLanguage:serpent"
  ],
  "main": "client.js",
  "contributes": {
    "languages": [
      {
        "id": "serpent",
        "aliases": [
          "Serpent"
        ],
        "extensions": [
          ".se"
        ],
        "configuration": "./serpent.configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "serpent",
        "scopeName": "source.serpent",
        "path": "./syntaxes/serpent.tmLanguage"
      }
    ]
  },
  "dependencies": {
    "vscode-languageclient": "^3.3.0",
    "vscode-languageserver": "^3.3.0"
  },
  "devDependencies": {
    "eslint": "^4.1.1",
    "vscode": "^1.1.0"
  }
}
